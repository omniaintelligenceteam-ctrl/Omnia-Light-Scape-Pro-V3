{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "functions": {
    "api/**/*.ts": {
      "runtime": "nodejs20.x",
      "maxDuration": 30
    }
  },
  "env": {
    "VITE_GEMINI_API_KEY": "@gemini-api-key",
    "VITE_SUPABASE_URL": "@supabase-url",
    "VITE_SUPABASE_SERVICE_KEY": "@supabase-service-key"
  }
}
